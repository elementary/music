#: data/io.elementary.music.appdata.xml.in:48
msgid "Fix list and column view sorting"
msgstr "Pataisytas sąrašo ir stulpelio rodinio rikiavimas"

#: data/io.elementary.music.appdata.xml.in:49
msgid "Add orange accent color"
msgstr "Pridėti oranžinę akcento spalvą"

#: data/io.elementary.music.appdata.xml.in:50
msgid "Add shortcut accels to tooltips"
msgstr "Į paaiškinimus pridėti susiejimai"

#: data/io.elementary.music.appdata.xml.in:57
msgid "Various UI changes"
msgstr "Įvairūs naudotojo sąsajos pakeitimai"

#: data/io.elementary.music.appdata.xml.in:58
msgid "Shuffle fixes"
msgstr "Maišymo pataisymai"

#: data/io.elementary.music.appdata.xml.in:59
msgid "Initial MPRIS support"
msgstr "Pradinis MPRIS palaikymas"

#: data/io.elementary.music.appdata.xml.in:66
msgid "Detect library changes at startup."
msgstr "Aptikti bibliotekos pakeitimus paleidimo metu."

#: data/io.elementary.music.appdata.xml.in:67
msgid "Support searching by URI"
msgstr "Palaikyti paiešką pagal URI"

#: data/io.elementary.music.appdata.xml.in:68
msgid "Change the queue mechanism to make it less confusing."
msgstr "Pakeistas eilės mechanizmas, kad būtų mažiau painus."

#: data/io.elementary.music.appdata.xml.in:69
msgid "Improve equalizer style."
msgstr "Patobulintas glodintuvo stilius."

#: data/io.elementary.music.appdata.xml.in:70
#: data/io.elementary.music.appdata.xml.in:75
#: data/io.elementary.music.appdata.xml.in:80
#: data/io.elementary.music.appdata.xml.in:85
msgid "Minor bug fixes."
msgstr "Nežymūs klaidų pataisymai."

#: data/io.elementary.music.appdata.xml.in:90
msgid "Add rounded bottom window corner."
msgstr "Pridėtas apvalusis apatinis lango kampas."

#: data/io.elementary.music.appdata.xml.in:93
#: data/io.elementary.music.appdata.xml.in:108
msgid "New translations"
msgstr "Nauji vertimai"

#: data/io.elementary.music.appdata.xml.in:99
msgid "Release for Loki."
msgstr "Laida, skirta Loki."

#: data/io.elementary.music.appdata.xml.in:101
msgid "Add support for Opus Ogg"
msgstr "Pridėtas Opus Ogg palaikymas"

#: data/io.elementary.music.appdata.xml.in:102
msgid "Fix Ctrl+Q support for non-Latin keyboard layouts"
msgstr ""
"Pataisytas Vald(Ctrl)+Q palaikymas nelotyniškiems klaviatūros išdėstymams"

#: data/io.elementary.music.appdata.xml.in:103
msgid "Reorganize media edition layout"
msgstr "Pertvarkytas medijos taisymo išdėstymas"

#: data/io.elementary.music.appdata.xml.in:104
msgid "Port grid to support HiDPI displays"
msgstr "Perkeltas tinklelis, kad būtų palaikomi HiDPI ekranai"

#: data/io.elementary.music.appdata.xml.in:105
msgid "Update icons to latest elementary icon release"
msgstr "Atnaujintos piktogramos į naujausią elementary piktogramų laidą"

#: data/io.elementary.music.appdata.xml.in:106
msgid "Respect privacy mode"
msgstr "Gerbti privatumo veikseną"

